{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "adamstipak/webpay-php": "dev-master",
        "barryvdh/laravel-translation-manager": "0.6.*",
        "bugsnag/bugsnag-laravel": "^2.0",
        "darryldecode/cart": "~4.0",
        "doctrine/dbal": "^3.0",
        "ecomailcz/ecomail": "^1.2",
        "endroid/qr-code": "^3.9",
        "heureka/overeno-zakazniky": "^3.0",
        "ibericode/vat": "*",
        "intervention/image": "^2.4",
        "laravel/framework": "10.41",
        "laravel/helpers": "*",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "5.11.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "6.4.*",
        "livewire/livewire": "^3.5.6",
        "lukesnowden/google-shopping-feed": "^2.1",
        "maatwebsite/excel": "~3.1.18",
        "mhujer/fio-api-php": "^4.0",
        "mikemclin/laravel-wp-password": "^2.0",
        "nesbot/carbon": "^2.0",
        "nunomaduro/collision": "^7.0",
        "openspout/openspout": "^4.24",
        "power-components/livewire-powergrid": "^6.0.0-beta5",
        "prologue/alerts": "^1.0.0",
        "sofort/sofortlib-php": "^3.3",
        "spatie/array-to-xml": "^2.7",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "6.4.0",
        "spipu/html2pdf": "^5.0",
        "srmklive/paypal": "^1.5",
        "stripe/stripe-php": "^7.2",
        "wireui/wireui": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
